<FindBugsFilter>
    <Match>
        <!-- Generated JPA classes. -->
        <Class name="~com\.cargopartner\.spot\.domainmodel\..*_"/>
        <Or>
            <Bug code="UuF"/>
            <Bug code="UwF"/>
            <Bug pattern="IL_INFINITE_RECURSIVE_LOOP"/>
        </Or>
    </Match>
    <Match>
        <Class name="~com\.cargopartner\.jshards\.backend\.domain\..*_"/>
        <Or>
            <Bug code="UuF"/>
            <Bug code="UwF"/>
        </Or>
    </Match>
    <Match>
        <!-- Reason - lot of views, forms, popups which are initialized with refresh/enter -->
        <Package name="~com\.cargopartner\.spot\.web.*"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    <Match>
        <!-- Customized vaadin components. -->
        <Package name="com.cargopartner.spot.web.core.ui.components"/>
        <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS"/>
    </Match>
    <Match>
        <!-- Unwanted checks -->
        <Or>
            <Bug pattern="NP_BOOLEAN_RETURN_NULL"/>
            <Bug pattern="EI_EXPOSE_REP"/>
            <Bug pattern="EI_EXPOSE_REP2"/>
            <Bug pattern="SE_NO_SERIALVERSIONID"/>
            <Bug pattern="SE_BAD_FIELD"/>
            <Bug pattern="SE_INNER_CLASS"/>
            <Bug pattern="IMA_INEFFICIENT_MEMBER_ACCESS"/>
            <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON"/>
            <Bug pattern="MS_MUTABLE_COLLECTION_PKGPROTECT"/>
            <Bug pattern="MS_MUTABLE_COLLECTION"/>
            <Bug pattern="WMI_WRONG_MAP_ITERATOR"/>
            <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
        </Or>
    </Match>
    <Match>
        <!-- jmockit mocked test code -->
        <Class name="~.+Test\$.+"/>
        <Bug code="UMAC"/>
    </Match>
</FindBugsFilter>
